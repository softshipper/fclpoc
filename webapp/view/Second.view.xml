		xmlns="sap.m"
		xmlns:f="sap.f"
		xmlns:layout="sap.ui.layout"
		height="100%">
		<f:DynamicPage>
			<!-- DynamicPage Title -->
			<f:title>
				<f:DynamicPageTitle>
					<f:heading>
						<Title text="Second"/>
					</f:heading>
				</f:DynamicPageTitle>
			</f:title>
			<!-- DynamicPage Header -->
			<f:header>
			</f:header>
			<f:content>
				<Text text="This is the second page"/>
			</f:content>
			<!-- DynamicPage Footer -->
			<f:footer>
			</f:footer>
		</f:DynamicPage>
</mvc:View>